local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local bannedHWID = "0954a9b5-ec26-400e-a342-2748a5a2fcd2"

if HWID == bannedHWID then
    print("Lave BannHub send to server!")
    wait(2)
    game.Players.LocalPlayer:Kick("You are permanently banned from Lave script hubs. [If this was a mistake, please let us know your Discord username: Lave_01]")
else
    print("Lave BannHub send to server!")
    wait(2)  
    print("You are not banned, have fun!") 
end
